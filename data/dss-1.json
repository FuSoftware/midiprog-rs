{
    "id" : "dss-1",
    "manufacturer" : "Korg",
    "name" : "DSS-1",
    "commands" : [
        {
            "name" : "Program Parameter Request",
            "midi" : "F0 42 3n 0B 10 p F7",
            "parameters" : [
                "n : 0.5 : Channel",
                "p : 1 : Parameter"
            ],
            "alias" : "pr"
        },
        {
            "name" : "Mode Request",
            "midi" : "F0 42 3n 0B 12 F7",
            "parameters" :  [
                "n : 0.5 : Channel"
            ],
            "alias" : "mr"
        },
        {
            "name" : "PCM Data Request",
            "midi" : "F0 42 3n 0B 14 a b F7",
            "parameters" :  [
                "n : 0.5 : Channel",
                "a : 6 : Start Address",
                "b : 6 : Last Address + 1"
            ],
            "alias" : "pcm"
        },
        {
            "name" : "Parameter Change",
            "midi" : "F0 42 3n 0B 41 p v F7",
            "parameters" :  [
                "n : 0.5 : Channel",
                "p : 1 : Parameter",
                "v : 1 : Value"
            ],
            "alias" : "pc param parameter parameter-change"
        },
        {
            "name" : "Mode Change",
            "midi" : "F0 42 3n 0B 42 m F7",
            "parameters" :  [
                "n : 0.5 : Channel",
                "m : 1 : Mode"
            ],
            "alias" : "mc mode mode-change"
        }
    ] 
}