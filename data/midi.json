{
    "id" : "midi",
    "manufacturer" : "Generic",
    "name" : "MIDI Standard",
    "commands" : [
        {
            "name" : "Note Off",
            "midi" : "8n k v",
            "parameters" : [
                "n : 0.5 : Channel",
                "k : 1 : Key",
                "v : 1 : Velocity"
            ],
            "alias" : "note-off note- noteoff noff n-"
        },
        {
            "name" : "Note On",
            "midi" : "9n k v",
            "parameters" : [
                "n : 0.5 : Channel",
                "k : 1 : Key",
                "v : 1 : Velocity"
            ],
            "alias" : "note-on note+ noteon non n+"
        },
        {
            "name" : "Polyphonic Key Pressure",
            "midi" : "An k v",
            "parameters" : [
                "n : 0.5 : Channel",
                "k : 1 : Key",
                "v : 1 : Velocity"
            ],
            "alias" : "polyphonic-key-pressure polykey poly pk"
        },
        {
            "name" : "Control Change",
            "midi" : "Bn c v",
            "parameters" : [
                "n : 0.5 : Channel",
                "c : 1 : Controller",
                "v : 1 : Value"
            ],
            "alias" : "control-change control cc"
        },
        {
            "name" : "Program Change",
            "midi" : "Cn p",
            "parameters" : [
                "n : 0.5 : Channel",
                "p : 1 : Program"
            ],
            "alias" : "program-change program pc"
        },
        {
            "name" : "Channel Pressure",
            "midi" : "Dn v",
            "parameters" : [
                "n : 0.5 : Channel",
                "v : 1 : Pressure"
            ],
            "alias" : "channel-pressure cp"
        },
        {
            "name" : "Pitch Bend Change",
            "midi" : "En l m",
            "parameters" : [
                "n : 0.5 : Channel",
                "l : 1 : LSB",
                "m : 1 : MSB"
            ],
            "alias" : "pitch-bend-change pitch pitch-bend pb"
        }
    ] 
}